<?xml version="1.0" encoding="utf-8"?>
<e107Theme name="Artemis" version="1.0.1" date="2021-06-21" compatibility="2.3.1">
	<author name ="Jimako" email="jimako@e107sk.com" url="https://www.e107sk.com" />
	<summary>Bootstrap4 generic theme (under development)</summary>
	<description>For testing only</description>
	<category>generic</category>
	<libraries>
		<library name="bootstrap" version="5" scope="front,wysiwyg"/>
		<library name="fontawesome" version="5" scope="front,wysiwyg"/>
	</libraries>
	<stylesheets>
		<css file="style.css" name="Default" exlude="bootstrap5"/>
	</stylesheets>
	<screenshots>
		<image>preview_artemis.jpg</image>
	</screenshots>
	<layouts>
		<layout name='home' title='Home Page' >
			<custompages>FRONTPAGE</custompages>
		</layout>
		<layout name='forum' title='Full Page for Forum' >
			<custompages>:forum</custompages>
		</layout>
			<layout name='full' title='Full Page' >
			<custompages>:download</custompages>
		</layout>
		<layout name='sidebar' title='Sidebar (right)'   />
		<layout name='singleform' title='Single form' >
			<custompages>login fpw signup</custompages>
		</layout>
		<layout name='contact' title='Contact page' >
			<custompages>contact</custompages>
		</layout>
        <layout name='default' title='Default layout' default='true' />
	</layouts>
</e107Theme>